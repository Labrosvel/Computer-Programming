DAYS_PER_MONTH = {
    'January': 31,
    'February': 28,
    'March': 31,
    'April': 30,
    'May': 31,
    'June': 30,
    'July': 31,
    'August': 31,
    'September': 30,
    'October': 31,
    'November': 30,
    'December': 31
}
    
month_name = input('Please insert the name of the month: ')
print(month_name, 'has', DAYS_PER_MONTH[month_name], 'days.')
