your_number = input("Please enter a number : ")
print("you entered ", your_number)
print("the mathematical operators applied to your number and 10 are:")
print(your_number, "+ 10 = ", int(your_number)+10)
print(your_number, "- 10 = ", int(your_number)-10)
print(your_number, "* 10 = ", int(your_number)*10)
print(your_number, "** 10 = ", int(your_number)**10)
print(your_number, "/ 10 = ", int(your_number)/10)
print(your_number, "% 10 = ", int(your_number)%10)
