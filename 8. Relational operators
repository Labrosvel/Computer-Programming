x = input("Please enter your first number : ")
y = input ("Please enter your second number : ")
print("You entered :", str(x), " and ", str(y))
print("The relational operators applying to your numbers are: ")
print("x = y is ", x == y )
print("x > y is ", x > y )
print("x != y is ", x != y )
print("x >= y is ", x >= y )
print("x < y is ", x <= y )
print("x <= y is ", x <= y )


